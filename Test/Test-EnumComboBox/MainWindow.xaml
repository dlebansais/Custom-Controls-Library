<Window x:Class="TestEnumComboBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ctrl="CustomControlsLibrary"
        Title="Test-EnumComboBox"
        WindowStartupLocation="CenterScreen"
        SizeToContent="WidthAndHeight">
    <DockPanel>
        <ctrl:EnumComboBox x:Name="ctrl" EnumBinding="{Binding TestProperty}" AutomationProperties.AutomationId="editableTextBlock"/>
    </DockPanel>
</Window>
